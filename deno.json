{
  "name": "ts-dsa",
  "version": "0.0.1",
  "tasks": {
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@utils": "./lib/utils/index.ts",
    "@data-structures": "./lib/data-structures/index.ts",
    "@algorithms": "./lib/algorithms/index.ts",
    "@problems": "./lib/problems/index.ts",
    "@lib": "./lib/index.ts"
